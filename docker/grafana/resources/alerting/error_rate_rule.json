{
  "title": "服务错误率过高",
  "ruleGroup": "service_error_rate",
  "folderUID": "tracelogkit",
  "noDataState": "OK",
  "execErrState": "Error",
  "for": "1m",
  "uid": "service_error_rate_5",
  "condition": "B",
  "annotations": {
    "summary": "服务 {{ $labels.job }} 错误率超过 5%（成功率低于 95%）",
    "description": "过去 5 分钟错误率 {{ $values.B.Value }}%，持续 1 分钟。"
  },
  "labels": {
    "severity": "critical"
  },
  "data": [
    {
      "refId": "A",
      "queryType": "",
      "relativeTimeRange": {
        "from": 300,
        "to": 0
      },
      "datasourceUid": "tracelogkit-prometheus",
      "model": {
        "expr": "100 * (sum by (job) (rate(http_server_requests_total{code=~\"[45]..\"}[5m])) / sum by (job) (rate(http_server_requests_total[5m])))",
        "hide": false,
        "intervalMs": 1000,
        "maxDataPoints": 43200,
        "refId": "A"
      }
    },
    {
      "refId": "B",
      "queryType": "",
      "relativeTimeRange": {
        "from": 0,
        "to": 0
      },
      "datasourceUid": "-100",
      "model": {
        "conditions": [
          {
            "evaluator": {
              "params": [5],
              "type": "gt"
            },
            "operator": {
              "type": "and"
            },
            "query": {
              "params": ["A"]
            },
            "reducer": {
              "params": [],
              "type": "last"
            },
            "type": "query"
          }
        ],
        "datasource": {
          "type": "__expr__",
          "uid": "-100"
        },
        "hide": false,
        "intervalMs": 1000,
        "maxDataPoints": 43200,
        "refId": "B",
        "type": "classic_conditions"
      }
    }
  ]
}
