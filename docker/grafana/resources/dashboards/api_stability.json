{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "datasource": { "type": "prometheus", "uid": "tracelogkit-prometheus" },
      "description": "Requests per second (all services).",
      "fieldConfig": { "defaults": { "unit": "reqps" }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "id": 1,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[1m])) by (service)",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ],
      "title": "RPS by service",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "tracelogkit-prometheus" },
      "description": "5xx error rate (all services).",
      "fieldConfig": { "defaults": { "unit": "percent", "min": 0, "max": 100 }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "id": 2,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "targets": [
        {
          "expr": "100 * (sum(rate(http_requests_total{status=~\"5..\"}[5m])) by (service) / sum(rate(http_requests_total[5m])) by (service))",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ],
      "title": "5xx error rate (%) by service",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "tracelogkit-prometheus" },
      "description": "p95 latency by service (bucket-based).",
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 8 },
      "id": 3,
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, service))",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ],
      "title": "HTTP p95 latency (seconds) by service",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "tracelogkit-prometheus" },
      "description": "In-flight requests by service.",
      "fieldConfig": { "defaults": { "unit": "short", "min": 0 }, "overrides": [] },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 17 },
      "id": 4,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "targets": [
        {
          "expr": "sum(http_requests_in_progress) by (service)",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ],
      "title": "HTTP in-progress by service",
      "type": "timeseries"
    }
  ],
  "refresh": "10s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["tracelogkit", "stability"],
  "templating": { "list": [] },
  "time": { "from": "now-15m", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "API Stability (HTTP)",
  "uid": "tracelogkit_api_stability",
  "version": 1
